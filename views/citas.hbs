<table border="solid">
  <h2>LISTADO DE CITAS</h2>
  <thead>
    <tr>
      <th>Fecha</th>
      <th>Paciente</th>
      <th>Medico</th>
      <th>Consultorio</th>
    </tr>
  </thead>
  <tbody>
    {{#each citas}}
      <tr>
        <td>{{formatoFecha fecha}}</td>
        <td>{{nombre}}</td>
        <td>{{nombres}}</td>
        <td>{{consultorio}}</td>
        <td><a
            class="eliminar"
            href="citas/eliminar/{{id}}"
          >Cancelar</a></td>
      </tr>
    {{/each}}
  </tbody>
</table>
<a href="/citas/agregar">Agregar cita medica</a>
<a href="/">Volver</a>
